{
  "name": "awesome-stars",
  "private": true,
  "version": "3.0.1",
  "description": "A Chrome extension that reveals star count of repositories on awesome list",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "lint": "standard",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "standard": {
    "globals": [
      "chrome"
    ],
    "parser": "babel-eslint"
  },
  "devDependencies": {
    "@storybook/addon-actions": "~3.3.12",
    "@storybook/addon-knobs": "~3.3.12",
    "@storybook/addons": "~3.3.12",
    "@storybook/react": "~3.3.12",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "7.x.x",
    "babel-loader": "~7.1.4",
    "babel-plugin-transform-class-properties": "~6.24.1",
    "babel-plugin-transform-object-rest-spread": "~6.26.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-preset-env": "1.x.x",
    "babel-preset-react": "~6.24.1",
    "chromereload": "0.x.x",
    "del": "3.x.x",
    "eslint": "~4.19.1",
    "eslint-config-standard": "~11.0.0",
    "eslint-config-standard-react": "~6.0.0",
    "eslint-loader": "2.x.x",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-node": "~5.2.1",
    "eslint-plugin-promise": "~3.6.0",
    "eslint-plugin-react": "~7.6.1",
    "eslint-plugin-standard": "~3.0.1",
    "file-loader": "~1.1.11",
    "gulp": "3.x.x",
    "gulp-bump": "2.x.x",
    "gulp-cache": "0.x.x",
    "gulp-clean-css": "~3.x.x",
    "gulp-filter": "5.x.x",
    "gulp-git": "2.x.x",
    "gulp-if": "2.x.x",
    "gulp-imagemin": "3.x.x",
    "gulp-json-transform": "0.x.x",
    "gulp-less": "3.x.x",
    "gulp-livereload": "3.x.x",
    "gulp-plumber": "1.x.x",
    "gulp-sass": "~3.x.x",
    "gulp-sequence": "0.x.x",
    "gulp-sourcemaps": "~2.x.x",
    "gulp-tag-version": "1.x.x",
    "gulp-util": "3.x.x",
    "gulp-zip": "4.x.x",
    "require-dir": "0.x.x",
    "uglifyjs-webpack-plugin": "~1.2.5",
    "vinyl-named": "1.x.x",
    "webpack": "4.x.x",
    "webpack-bundle-analyzer": "~2.11.1",
    "webpack-stream": "4.x.x",
    "yargs": "9.x.x"
  },
  "dependencies": {
    "apollo-boost": "~0.1.6",
    "awilix": "~3.0.5",
    "axios": "~0.21.1",
    "axios-extensions": "~3.0.2",
    "bluebird": "~3.5.0",
    "chomex": "~1.0.5",
