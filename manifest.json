{
  "background": {
    "scripts": [
      "utils.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/awesome-stars.png",
    "default_title": "Awesome stars"
  },
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "js": [
        "libs/jquery.min.js",
        "utils.js",
        "content.js"
      ],
      "matches": [
        "https://github.com/*"
      ]
    }
  ],
  "description": "Peek stars of repository on awesome list.",
  "icons": {
    "128": "assets/awesome-stars.png"
  },
  "manifest_version": 2,
  "name": "Awesome stars",
  "permissions": [
    "tabs",
    "storage"
  ],
  "version": "1.0.0"
}
